function printF(){
    let pattern ="";
    for(let i=1; i<=7;i++){
        for(let j=1;j<=5;j++){
            if(j == 1 || j + i == 6 || i - j == 2)
            {
                pattern += "* ";
            }else{
                pattern += " ";
            }
        }
        pattern += "\n";
    }
    console.log(pattern);
}
printF();
